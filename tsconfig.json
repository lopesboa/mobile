{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "baseUrl": "./"
    // "paths": {
    //   "@types/coorp/*": ["src/@types/coorpacademy/*"]
    // }
  },
  "exclude": [
    "node_modules",
    "**/**.js",
    "babel.config.js",
    "metro.config.js",
    "jest.config.unit.js"
  ]
}
